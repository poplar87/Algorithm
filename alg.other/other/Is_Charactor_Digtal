#include "stdafx.h"
#include <stdlib.h>
#include <stdio.h>
#include <string>
#include <iostream>

using namespace std;

long strtoint(char *str,int length);

int main(int argc, char* argv[])
{
    int i=0;
 char str[100];
 while ((str[i]=getchar())!='0') {
  i++;
 }
 
long n=strtoint(str,i);

 cout<<n<<endl;
 
 return 0;
}

long stringToInt(char *str,int length)  
{  
    if(length==1)
        return str[0]=='-' ? 0 : str[0]-'0';  
     return str[0]=='-' ? stringToInt(str, length-1)*10-(str[length-1]-'0') : stringToInt(str, length-1)*10+str[length-1]-'0';  
}
